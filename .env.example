# AgentMove Environment Configuration
# Copy this file to .env and fill in your API keys

# ============================================
# LLM API Keys
# ============================================

# SiliconFlow API (Free tokens for 7B models: Qwen, GLM, DeepSeek)
# Get your key at: https://siliconflow.cn
SiliconFlow_API_KEY=your_siliconflow_api_key_here

# DeepInfra API (Llama, Gemma, Mistral models)
# Get your key at: https://deepinfra.com
DeepInfra_API_KEY=your_deepinfra_api_key_here

# OpenAI API (GPT models)
# Get your key at: https://platform.openai.com
OpenAI_API_KEY=your_openai_api_key_here

# OpenRouter API (Alternative OpenAI-compatible API)
# Get your key at: https://openrouter.ai
OpenRouter_API_KEY=your_openrouter_api_key_here

# vLLM API (Local deployment)
# If using local vLLM deployment, set this key
vllm_KEY=EMPTY

# ============================================
# Address Resolution Service
# ============================================

# Nominatim Server Address (for address resolution)
# Format: IP:Port, e.g., 127.0.0.1:8080
# Deploy using: https://github.com/mediagis/nominatim-docker
nominatim_deploy_server_address=127.0.0.1:8080

# ============================================
# Proxy Configuration (Optional)
# ============================================

# HTTP Proxy for API requests (if needed)
# Format: http://host:port
# Leave empty if not using proxy
PROXY=

# ============================================
# Demo Configuration (Optional)
# ============================================

# Demo server host (default: 0.0.0.0)
DEMO_HOST=0.0.0.0

# Demo server port (default: 8000)
DEMO_PORT=8000

# Default city for demo (default: Shanghai)
DEMO_DEFAULT_CITY=Shanghai

# Default model for demo (default: qwen2.5-7b)
DEMO_DEFAULT_MODEL=qwen2.5-7b

# Default platform for demo (default: SiliconFlow)
DEMO_DEFAULT_PLATFORM=SiliconFlow
